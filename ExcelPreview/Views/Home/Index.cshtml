@{
    ViewBag.Title = "Excel Upload";
}

<div class="container">
    <h3>Excel Upload</h3>

    <div class="row">
        <div class="col-md-8">
            @using (Html.BeginForm("Index", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <input type="file" name="postedFile" />
                <br />
                <input type="submit" value="Upload Excel" />
            }
        </div>
    </div>
</div>

